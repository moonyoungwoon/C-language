#include <stdio.h>

int main(void)
{
    int arr[5];
    int max, min, total;

    printf("5개의 정수 입력>> ");
    scanf_s("%d", &arr[0]);
    max = min = total = arr[0];

    for (int i = 1; i < 5; i++)
    {
        scanf_s("%d", &arr[i]);

        if (max < arr[i])
            max = arr[i];
        if (min > arr[i])
            min = arr[i];
        total += arr[i];
    }

    printf("입력된 정수 중에서 최댓값: %d\n", max);
    printf("입력된 정수 중에서 최솟값: %d\n", min);
    printf("입력된 정수의 총합: %d\n", total);

    return 0;
}
